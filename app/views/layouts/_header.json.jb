# TODO: Add updates and more into session

json = {}

json_token = current_endpoint&.new_token || current_user&.new_token
if json_token.present?
  json[:session] = {
    token: json_token
  }
end

unless @pagy.nil?
  json[:page] = @pagy.page
  json[:pages] = @pagy.pages
  json[:from] = @pagy.from
  json[:to] = @pagy.to
end

json
