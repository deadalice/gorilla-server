if current_user&.endpoint&.new_token.present?
  json = {
    session: {
      token: current_user.endpoint.new_token
    }
  }
else
  json = {}
end

unless @pagy.nil?
  json[:page] = @pagy.page
  json[:pages] = @pagy.pages
  json[:from] = @pagy.from
  json[:to] = @pagy.to
end

json
