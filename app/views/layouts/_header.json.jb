# TODO: Add updates and more into session

json = current_endpoint&.new_token.present? ? {
  session: {
    token: current_endpoint.new_token,
  },
} : {}

unless @pagy.nil?
  json[:page] = @pagy.page
  json[:pages] = @pagy.pages
  json[:from] = @pagy.from
  json[:to] = @pagy.to
end

json
