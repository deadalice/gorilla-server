json = {
  name: (package.alias || package.name),
  text: package.text,
  key: package.key,
  version: package.version,
  filename: package.filename,
  checksum: package.checksum,
  discarded: package.discarded?,
  trusted: package.trusted,
  created_at: package.created_at.to_i,
  updated_at: package.updated_at.to_i

  # TODO: Icon to Product! icon: url_for(package.icon)
}
